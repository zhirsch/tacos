diff -Naur gcc-4.9.1.orig/gcc/config/i386/i386tacos.h gcc-4.9.1/gcc/config/i386/i386tacos.h
--- gcc-4.9.1.orig/gcc/config/i386/i386tacos.h	1969-12-31 16:00:00.000000000 -0800
+++ gcc-4.9.1/gcc/config/i386/i386tacos.h	2014-08-11 10:35:51.476410486 -0700
@@ -0,0 +1,8 @@
+#undef TARGET_OS_CPP_BUILTINS
+#define TARGET_OS_CPP_BUILTINS()      \
+  do {                                \
+    builtin_define_std ("tacos");     \
+    builtin_define_std ("unix");      \
+    builtin_assert ("system=tacos");  \
+    builtin_assert ("system=unix");   \
+  } while(0);
diff -Naur gcc-4.9.1.orig/gcc/config.gcc gcc-4.9.1/gcc/config.gcc
--- gcc-4.9.1.orig/gcc/config.gcc	2014-06-16 01:15:35.000000000 -0700
+++ gcc-4.9.1/gcc/config.gcc	2014-08-11 10:35:51.476410486 -0700
@@ -867,6 +867,12 @@
     *) echo 'Unknown thread configuration for VxWorks'; exit 1 ;;
   esac
   ;;
+*-*-tacos*)
+  gas=yes
+  gnu_ld=yes
+  # Assume that newlib is being used and so __cxa_atexit is provided.
+  default_use_cxa_atexit=yes
+  ;;
 *-*-elf|arc*-*-elf*)
   # Assume that newlib is being used and so __cxa_atexit is provided.
   default_use_cxa_atexit=yes
@@ -1342,6 +1348,9 @@
 i[34567]86-*-elf*)
 	tm_file="${tm_file} i386/unix.h i386/att.h dbxelf.h elfos.h newlib-stdint.h i386/i386elf.h"
 	;;
+i[34567]86-*-tacos*)
+	tm_file="${tm_file} i386/unix.h i386/att.h dbxelf.h elfos.h newlib-stdint.h i386/i386elf.h i386/i386tacos.h"
+	;;
 x86_64-*-elf*)
 	tm_file="${tm_file} i386/unix.h i386/att.h dbxelf.h elfos.h newlib-stdint.h i386/i386elf.h i386/x86-64.h"
 	;;
